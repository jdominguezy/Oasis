@model  Oasis.Models.Login.LoginView

@{
    Layout = null;
    ViewBag.Title = "Login";
}


<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Oasis | Login</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Ionicons -->
    @*<link rel="stylesheet" href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css">*@
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/FrontEnd/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/summernote/summernote-bs4.min.css">
    <!-- swal -->
    <link rel="stylesheet" href="~/Content/FrontEnd/plugins/sweetalert2/sweetalert2.min.css">
    <!--select2-->
    <link href="~/Content/css/select2.min.css" rel="stylesheet" />
    <!-- Import D3 Scale Chromatic via CDN -->
    <script src="https://d3js.org/d3-color.v2.min.js"></script>
    <script src="https://d3js.org/d3-interpolate.v2.min.js"></script>
    <script src="https://d3js.org/d3-scale-chromatic.v2.min.js"></script>
    @*DatatableJS*@
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.10.24/b-1.7.0/b-colvis-1.7.0/b-html5-1.7.0/b-print-1.7.0/cr-1.5.3/kt-2.6.1/r-2.2.7/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/FrontEnd/dist/css/login_main.css">
    <link rel="stylesheet" type="text/css"  href="~/Content/FrontEnd/dist/css/login_util.css">
</head>
<body style="min-height: 496.391px;">
    <div class="container-login100" style="background-image: url('../../Content/FrontEnd/dist/img/Edificio-BETA-GV.jpg');">
        <div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">

            @using (Html.BeginForm(null, null, new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post,
    new { @class = "login100-form validate-form" }))
            {

                @Html.AntiForgeryToken()
                <span class="login100-form-title p-b-37">
                    Iniciar sesión
                </span>
                <div class="wrap-input100 validate-input m-b-20" data-validate="Enter username or email">
                    @Html.EditorFor(model => model.username, new { htmlAttributes = new { @class = "input100", @placeholder = "Usuario" } })
                    @Html.ValidationMessageFor(model => model.username, "", new { @class = "text-danger" })
                    <span class="focus-input100"></span>
                </div>

                <div class="wrap-input100 validate-input m-b-25" data-validate="Enter password">
                    @Html.EditorFor(model => model.password, new { htmlAttributes = new { @class = "input100", @type = "password", @placeholder = "Password" } })
                    @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })
                    <span class="focus-input100"></span>
                </div>
                <div style="display:flex;justify-content:center;">@Html.ValidationMessage("", new { @style = "font-size:20px; font-weight:600;" })</div>
                <div class="container-login100-form-btn">
                    <button class="login100-form-btn">
                        Ingresar
                    </button>
                </div>


            }
        </div>
    </div>



    <div id="dropDownSelect1"></div>
    <!-- /.login-box -->
    <!-- jQuery -->
    <script src="../../plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="../../plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="../../dist/js/adminlte.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>